{
  "name": "request-marketplace-backend",
  "version": "1.0.0",
  "description": "PostgreSQL-based backend API for Request Marketplace",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "deploy:scp:ps": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-scp.ps1",
    "deploy:scp:sh": "bash ./scripts/deploy-scp.sh",
    "migrate:promos": "node ./scripts/run-sql-migration.js ./migration/20250829_promo_codes.sql",
    "migrate:subscription-mappings": "node ./scripts/run-sql-migration.js ./migration/20250831_subscription_mappings.sql",
    "migrate:sms-config": "node ./scripts/run-sql-migration.js ./migrations/20250831_add_sms_config_approval_columns.sql",
    "seed:categories": "node ./scripts/reseed_categories_v2.js",
    "backfill:category:request_type": "node ./scripts/backfill_category_request_type.js",
    "migrate:ssh": "powershell -ExecutionPolicy Bypass -File ./scripts/run-migration-ssh.ps1",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
  "schema:audit": "node tools/schema_audit.js",
  "migrate:hiring-to-job": "node ./scripts/run-migration-iam.js ./database/migrations/034_rename_hiring_to_job.sql"
  },
  "keywords": [
    "nodejs",
    "express",
    "postgresql",
    "api",
    "marketplace"
  ],
  "author": "Request Marketplace Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/client-ses": "^3.645.0",
    "@aws-sdk/client-sns": "^3.645.0",
    "@aws-sdk/rds-signer": "^3.879.0",
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "helmet": "^7.2.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.4",
    "pg": "^8.11.3",
    "twilio": "^5.8.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
